{
    "name": "camelot/api-platform-rate-limiter",
    "description": "Rate limiting support for API Platform",
    "type": "library",
    "require": {
        "php": "^8.1",
        "symfony/lock": "^6.4 || ^7.0",
        "symfony/rate-limiter": "^6.4 || ^7.0"
    },
    "require-dev": {
        "api-platform/core": "^3.3.x-dev",
        "camelot/coding-style": "^3.0",
        "friendsofphp/php-cs-fixer": "^3.21",
        "phpunit/phpunit": "^10.5",
        "symfony/dependency-injection": "^6.4 || ^7.0",
        "symfony/filesystem": "^6.4 || ^7.0",
        "symfony/framework-bundle": "^6.4 || ^7.0",
        "symfony/runtime": "^6.4 || ^7.0",
        "symfony/security-core": "^6.4 || ^7.0",
        "symfony/yaml": "^6.4 || ^7.0",
        "symfony/browser-kit": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/asset": "^7.0",
        "symfony/http-client": "^7.0",
        "symfony/security-bundle": "^7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gwendolen Lynch",
            "email": "gwendolen.lynch@gamil.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Camelot\\RateLimiter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Camelot\\RateLimiter\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
